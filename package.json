{
  "name": "graphql-training-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env  nodemon --ext ts,json,graphql --exec babel-node --extensions '.ts' src/index.ts",
    "test": "yarn lint && yarn tsc && yarn test-all",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc",
    "test-all": "yarn test-unit && yarn test-integration",
    "test-unit": "jest -c __tests__/jest.config.unit.js",
    "test-integration": "jest -c __tests__/jest.config.integration.js --runInBand",
    "build": "yarn build:clean && yarn build:babel",
    "build:babel": "babel src --out-dir build --copy-files --extensions='.ts'",
    "build:clean": "rm -rf build",
    "knex": "knex --env default",
    "knex-test": "knex --env test",
    "run-script": "cross-env babel-node --extensions '.ts'"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.7",
    "apollo": "^2.21.1",
    "apollo-engine": "^1.1.2",
    "apollo-server-core": "^2.9.12",
    "apollo-server-express": "^2.9.12",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dataloader": "^2.0.0",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "finnish-ssn": "^2.0.4",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "import-sort-style-module": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.3",
    "ksuid": "^1.2.0",
    "libphonenumber-js": "^1.7.40",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.7.3",
    "pg": "^7.14.0",
    "sql-template-strings": "^2.2.2",
    "ts-custom-error": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.14",
    "@types/pg": "^7.11.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "^24.9.0",
    "ksuid-cli": "^1.1.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2"
  }
}
