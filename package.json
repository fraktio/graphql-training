{
  "name": "enjoy-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env nodemon --ext ts,json,graphql --exec babel-node --extensions '.ts' src/index.ts",
    "test": "yarn lint && yarn tsc",
    "lint": "tslint 'src/**/*.ts'",
    "tsc": "tsc",
    "build": "yarn build:clean && yarn build:babel",
    "build:babel": "babel src --out-dir build --copy-files --extensions='.ts'",
    "build:clean": "rm -rf build",
    "knex": "knex --env default",
    "knex-test": "knex --env test"
  },
  "dependencies": {
    "apollo": "^2.4.4",
    "apollo-engine": "^1.1.2",
    "apollo-server-express": "^2.4.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "knex": "^0.16.3",
    "merge-graphql-schemas": "^1.5.8",
    "pg": "^7.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.0.5",
    "@types/node": "^10.12.23",
    "@types/pg": "^7.4.11",
    "babel-plugin-module-resolver": "^3.1.3",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  }
}
