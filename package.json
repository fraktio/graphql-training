{
  "name": "enjoy-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env nodemon --ext ts,json,graphql --exec babel-node --extensions '.ts' src/index.ts",
    "test": "yarn lint && yarn tsc && yarn test-all",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc",
    "test-all": "yarn test-unit && yarn test-integration",
    "test-unit": "jest -c __tests__/jest.config.unit.js",
    "test-integration": "jest -c __tests__/jest.config.integration.js --runInBand",
    "build": "yarn build:clean && yarn build:babel",
    "build:babel": "babel src --out-dir build --copy-files --extensions='.ts'",
    "build:clean": "rm -rf build",
    "knex": "knex --env default",
    "knex-test": "knex --env test",
    "run-script": "cross-env babel-node --extensions '.ts'",
    "create-provider": "yarn run-script src/script/create-provider/index.ts"
  },
  "dependencies": {
    "apollo": "^2.5.1",
    "apollo-engine": "^1.1.2",
    "apollo-server-express": "^2.4.6",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dataloader": "^1.4.0",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "finnish-business-ids": "^3.1.0",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "knex": "^0.16.3",
    "ksuid": "^1.1.3",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "pg": "^7.8.1",
    "sql-template-strings": "^2.2.2",
    "ts-custom-error": "^2.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.122",
    "@types/node": "^11.9.5",
    "@types/pg": "^7.4.13",
    "babel-jest": "^24.3.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "^24.3.1",
    "ksuid-cli": "^1.1.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3333"
  }
}
